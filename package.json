{
   "name": "electron-list",
   "author": "Test",
   "version": "0.1.0",
   "private": true,
   "main": "public/main.js",
   "homepage": ".",
   "build": {
      "appId": "com.example.electron-cra",
      "files": [
         "build/**/*",
         "node_modules/**/*"
      ],
      "directories": {
         "buildResources": "assets"
      }
   },
   "dependencies": {
      "concurrently": "^5.0.0",
      "cross-env": "^6.0.3",
      "electron-is-dev": "^1.1.0",
      "is-electron": "^2.2.0",
      "react": "^16.10.2",
      "react-dom": "^16.10.2",
      "react-router-dom": "^5.1.2",
      "react-scripts": "3.2.0",
      "styled-components": "^4.4.0"
   },
   "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "waitStart": "concurrently \"cross-env BROWSER=none npm run react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
      "electron-start": "electron .",
      "preelectron-pack": "npm run build",
      "electron-pack": "build --win --macos -c.extraMetadata.main=build/Main.js",
      "pack": "electron-builder --dir",
      "dist": "npx build --x64 --macos --win --linux --c.extraMetadata.main=build/electron.js -p always",
      "test": "react-scripts test",
      "eject": "react-scripts eject"
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "devDependencies": {
      "electron": "^6.0.12",
      "electron-builder": "^21.2.0"
   }
}
